{
  "settings": {
    "analysis": {
      "analyzer": {
        "custom_analyzer": {
          "type": "custom",
          "tokenizer": "whitespace",
          "filter": ["lowercase"]
        }
      }
    }
  },

    "mappings": {
      "properties": {
        "title":          { "type": "text"    },
        "composer":       { "type": "keyword" },
        "key":            { "type": "keyword" },
        "time_signature": { "type": "keyword" },
        "tokens": {
          "type": "text",
          "analyzer": "custom_analyzer"
        },
        "interval_fp": {
          "type": "text",
          "analyzer": "custom_analyzer"
        },
        "rhythm_fp": {
          "type": "text",
          "analyzer": "custom_analyzer"
        }
      }
    }
  }
  